  "scripts": {
    "ng": "ng",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "mkv-app": "npx nx serve mkv-app",
    "mk-admin-app": "npx nx serve mk-admin-app",
    "mkod-app": "npx nx serve mkod-app",
    "build:a300:mk-admin-app": "npx nx build mk-admin-app -c a300 --output-hashing=all --base-href /mk-admin-app --deploy-url mk-admin-app/ --skip-nx-cache",
    "build:a300:mkv-app": "npx nx build mkv-app -c a300 --output-hashing=all --base-href /mkv-app --deploy-url mkv-app/ --skip-nx-cache",
    "build:a300:mkod-app": "npx nx build mkod-app -c a300 --output-hashing=all --base-href /mkod-app --deploy-url mkod-app/ --skip-nx-cache",
    "build:prod:mk-admin-app": "nx build mk-admin-app --configuration production --output-hashing=all --base-href /mk-admin-app --deploy-url mk-admin-app/ --skip-nx-cache",
    "build:prod:mkv-app": "nx build mkv-app --configuration production --output-hashing=all --base-href /mkv-app --deploy-url mkv-app/ --skip-nx-cache",
    "build:prod:mkod-app": "nx build mkod-app --configuration production --output-hashing=all --base-href /mkod-app --deploy-url mkod-app/ --skip-nx-cache",
    "test": "npx nx test",
    "test:all": "npx nx run-many --target=test --all",
    "lint:all": "npx nx run-many --target=lint --all",
    "full-check": "npx nx run-s lint:all test:all build"
  },
  "private": true,
