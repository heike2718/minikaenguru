    "mkv-app": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/mkv-app",
      "sourceRoot": "apps/mkv-app/src",
      "prefix": "mkv",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/mkv-app",
            "index": "apps/mkv-app/src/index.html",
            "main": "apps/mkv-app/src/main.ts",
            "polyfills": "apps/mkv-app/src/polyfills.ts",
            "tsConfig": "apps/mkv-app/tsconfig.app.json",
            "assets": [
              "apps/mkv-app/src/favicon.ico",
              "apps/mkv-app/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap/dist/css/bootstrap-grid.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "apps/mkv-app/src/styles.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mkv-app/src/app/store-config/store-devtools.ts",
                  "with": "apps/mkv-app/src/app/store-config/store-devtools.prod.ts"
                },
                {
                  "replace": "apps/mkv-app/src/environments/environment.ts",
                  "with": "apps/mkv-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
			},
            "a300": {
              "fileReplacements": [
                {
                  "replace": "apps/mkv-app/src/environments/environment.ts",
                  "with": "build-env/mkv-app/environment-a300.ts"
                },
                {
                  "replace": "apps/mkv-app/src/app/store-config/store-devtools.ts",
                  "with": "build-env/store-devtools.a300.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mkv-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mkv-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mkv-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/mkv-app/tsconfig.app.json",
              "apps/mkv-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/mkv-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mkv-app/jest.config.js",
            "tsConfig": "apps/mkv-app/tsconfig.spec.json",
            "setupFile": "apps/mkv-app/src/test-setup.ts",
            "passWithNoTests": true
          }
        }
      }
    },
